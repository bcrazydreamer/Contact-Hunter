{
  "name": "Contact-Hunter-1",
  "description": "This app will find and show all phone number with country detail and emails",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "<all_urls>"],
  "browser_action": {
      "default_title": "Contact-Hunter",
      "default_icon": "./img/logo.png",
      "default_popup": "popup.html"
  },
  "icons": {
    "16": "./img/icon-16.png",
    "48": "./img/icon-48.png",
    "128": "./img/icon-128.png"
  },
  "manifest_version": 2
}